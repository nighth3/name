sbar1.Parent = Main
sbar1.BackgroundColor3 = Color3.new(0.0588235, 0.0980392, 0.0588235)
sbar1.Position = UDim2.new(0.0175438598, 0, 0.0270270295, 0)
sbar1.Size = UDim2.new(0, 330, 0, 32)

grabknifev3.Name = "grabknifev3"
grabknifev3.Parent = Main
grabknifev3.BackgroundColor3 = Color3.new(0.0980392, 0.0980392, 0.0980392)
grabknifev3.BorderSizePixel = 0
grabknifev3.Position = UDim2.new(0.023391813, 0, 0.907335937, 0)
grabknifev3.Size = UDim2.new(0, 329, 0, 19)
grabknifev3.Font = Enum.Font.SourceSans
grabknifev3.Text = "GRAB KNIFE V3"
grabknifev3.TextScaled = true
