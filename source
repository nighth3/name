infiniteyield.Parent = Main
infiniteyield.BackgroundColor3 = Color3.new(0.0980392, 0.0980392, 0.0980392)
infiniteyield.BorderSizePixel = 0
infiniteyield.Position = UDim2.new(0.0263157897, 0, 0.799227774, 0)
infiniteyield.Size = UDim2.new(0, 329, 0, 19)
infiniteyield.Font = Enum.Font.SourceSans
infiniteyield.Text = "Infinite Yield 7.1"
infiniteyield.TextScaled = true
infiniteyield.TextSize = 14
infiniteyield.TextWrapped = true
infiniteyield.MouseButton1Down:connect(function()
---------------------------------------------------------------
--  _____       __ _       _ _        __   ___      _     _  --
-- |_   _|     / _(_)     (_) |       \ \ / (_)    | |   | | --
--   | | _ __ | |_ _ _ __  _| |_ ___   \ V / _  ___| | __| | --
--   | || '_ \|  _| | '_ \| | __/ _ \   \ / | |/ _ \ |/ _` | --
--  _| || | | | | | | | | | | ||  __/   | | | |  __/ | (_| | --
--  \___/_| |_|_| |_|_| |_|_|\__\___|   \_/ |_|\___|_|\__,_| --
--                                                           --
