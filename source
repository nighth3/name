OpenOrClose.BorderSizePixel = 0
OpenOrClose.Position = UDim2.new(0.00315059861, 0, 0.495735615, 0)
OpenOrClose.Size = UDim2.new(0, 100, 0, 100)

Open.MouseButton1Down:connect(function()
Main.Visible = true
Open.Visible = false
Close.Visible = true
end)
Open.Name = "Open"
Open.Parent = OpenOrClose
Open.BackgroundColor3 = Color3.new(0.0980392, 0.0980392, 0.0980392)
Open.BorderSizePixel = 0
